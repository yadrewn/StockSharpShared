<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="stocksharp" type="StockSharp.Studio.StrategyRunner.Configuration.StockSharpSection, StrategyRunner"/>
	</configSections>

	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
	</startup>

	<stocksharp>
		<connections>
			<connection transactionAdapter="StockSharp.AlfaDirect.AlfaDirectMessageAdapter, StockSharp.AlfaDirect" marketDataAdapter="StockSharp.AlfaDirect.AlfaDirectMessageAdapter, StockSharp.AlfaDirect"/>
			<connection transactionAdapter="StockSharp.BitStamp.BitStampMessageAdapter, StockSharp.BitStamp" marketDataAdapter="StockSharp.BitStamp.BitStampMessageAdapter, StockSharp.BitStamp"/>
			<connection transactionAdapter="StockSharp.Blackwood.BlackwoodMessageAdapter, StockSharp.Blackwood" marketDataAdapter="StockSharp.Blackwood.BlackwoodMessageAdapter, StockSharp.Blackwood"/>
			<connection transactionAdapter="StockSharp.Btce.BtceMessageAdapter, StockSharp.Btce" marketDataAdapter="StockSharp.Btce.BtceMessageAdapter, StockSharp.Btce"/>
			<connection transactionAdapter="StockSharp.CQG.CQGMessageAdapter, StockSharp.CQG" marketDataAdapter="StockSharp.CQG.CQGMessageAdapter, StockSharp.CQG"/>
			<connection transactionAdapter="StockSharp.ETrade.ETradeMessageAdapter, StockSharp.ETrade" marketDataAdapter="StockSharp.ETrade.ETradeMessageAdapter, StockSharp.ETrade"/>
			<connection transactionAdapter="StockSharp.Fix.FixMessageAdapter, StockSharp.Fix" marketDataAdapter="StockSharp.Fix.FixMessageAdapter, StockSharp.Fix"/>
			<connection transactionAdapter="StockSharp.InteractiveBrokers.InteractiveBrokersMessageAdapter, StockSharp.InteractiveBrokers" marketDataAdapter="StockSharp.InteractiveBrokers.InteractiveBrokersMessageAdapter, StockSharp.InteractiveBrokers"/>
			<connection marketDataAdapter="StockSharp.IQFeed.IQFeedMarketDataMessageAdapter, StockSharp.IQFeed"/>
			<connection transactionAdapter="StockSharp.LMAX.LmaxMessageAdapter, StockSharp.LMAX" marketDataAdapter="StockSharp.LMAX.LmaxMessageAdapter, StockSharp.LMAX"/>
			<connection transactionAdapter="StockSharp.Micex.MicexMessageAdapter, StockSharp.Micex" marketDataAdapter="StockSharp.Micex.MicexMessageAdapter, StockSharp.Micex"/>
			<connection transactionAdapter="StockSharp.Oanda.OandaMessageAdapter, StockSharp.Oanda" marketDataAdapter="StockSharp.Oanda.OandaMessageAdapter, StockSharp.Oanda"/>
			<connection transactionAdapter="StockSharp.OpenECry.OpenECryMessageAdapter, StockSharp.OpenECry" marketDataAdapter="StockSharp.OpenECry.OpenECryMessageAdapter, StockSharp.OpenECry"/>
			<connection transactionAdapter="StockSharp.Plaza.PlazaTransactionMessageAdapter, StockSharp.Plaza" marketDataAdapter="StockSharp.Plaza.PlazaStreamMessageAdapter, StockSharp.Plaza"/>
			<connection transactionAdapter="StockSharp.Quik.QuikTrans2QuikAdapter, StockSharp.Quik" marketDataAdapter="StockSharp.Quik.QuikDdeAdapter, StockSharp.Quik"/>
			<connection transactionAdapter="StockSharp.Rithmic.RithmicMessageAdapter, StockSharp.Rithmic" marketDataAdapter="StockSharp.Rithmic.RithmicMessageAdapter, StockSharp.Rithmic"/>
			<connection marketDataAdapter="StockSharp.Rss.RssMarketDataMessageAdapter, StockSharp.Rss"/>
			<connection transactionAdapter="StockSharp.SmartCom.SmartComMessageAdapter, StockSharp.SmartCom" marketDataAdapter="StockSharp.SmartCom.SmartComMessageAdapter, StockSharp.SmartCom"/>
			<connection transactionAdapter="StockSharp.Sterling.SterlingMessageAdapter, StockSharp.Sterling" marketDataAdapter="StockSharp.Sterling.SterlingMessageAdapter, StockSharp.Sterling"/>
			<connection transactionAdapter="StockSharp.Transaq.TransaqMessageAdapter, StockSharp.Transaq" marketDataAdapter="StockSharp.Transaq.TransaqMessageAdapter, StockSharp.Transaq"/>
		</connections>
		<diagramElements>
			<element type="StockSharp.Xaml.Diagram.Elements.CandleSourceDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.ChartDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.IndicatorDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.ComparisonDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.LogicalConditionDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.OpenPositionDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.ProtectPositionDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.OrderTradesDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.PassThroughDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.PreviousValueDiagramElement, StockSharp.Xaml.Diagram"/>
			<!--<element type="StockSharp.Xaml.Diagram.Elements.AlertDiagramElement, StockSharp.Xaml.Diagram"/>-->
			<element type="StockSharp.Xaml.Diagram.Elements.PositionDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.SecurityDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.MarketDepthDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.VariableDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.OneParamFunctionDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.TwoParamFunctionDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.PartDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.IndexerDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.LogicOneParamFunctionDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.StrategyTradesDiagramElement, StockSharp.Xaml.Diagram"/>
			<element type="StockSharp.Xaml.Diagram.Elements.CheckWorkingTimeDiagramElement, StockSharp.Xaml.Diagram"/>
		</diagramElements>
	</stocksharp>
</configuration>